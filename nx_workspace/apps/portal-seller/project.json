{
  "name": "portal-seller",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portal-seller",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project portal-seller --web",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/portal-seller",
        "command": "next build"
      },
      "outputs": [
        "{workspaceRoot}/apps/portal-seller/.next"
      ],
      "dependsOn": ["^build"],
      "cache": true
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/portal-seller",
        "command": "next start --port=3001"
      },
      "dependsOn": ["build"],
      "parallelism": true
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/portal-seller",
        "command": "next start --port=3001"
      },
      "dependsOn": ["build"],
      "parallelism": true
    }
  }
}
